{
  "manifest_version": 3,
  "name": "SeaDesk",
  "version": "1.0",
  "description": "A calming interactive beach and ocean experience with soothing sounds and animations.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "assets/fish2.png"
    }
  },
  "icons": {
    "48": "assets/fish2.png",
    "128": "assets/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*.png",
        "assets/*.mp3"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": []
}
